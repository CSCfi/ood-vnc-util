[Desktop Entry]
Type=Application
Name=Zonation
Terminal=true
<% if context.desktop == "mate" %>
Exec=bash -c 'ssh -t $HOSTNAME "export DISPLAY=$DISPLAY;export TMPDIR=$TMPDIR;export XDG_RUNTIME_DIR=$XDG_RUNTIME_DIR;module load zonation;z5"'
<%else%>
Exec=module load zonation;z5
<%end%>
Icon=<%= File.join(session.staged_root, "icons", "zonation.png") -%>
