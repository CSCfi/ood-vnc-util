[Desktop Entry]
Type=Application
Name=CloudCompare
Terminal=true
<% if context.desktop == "mate" %>
Exec=bash -c 'ssh -t $HOSTNAME "export DISPLAY=$DISPLAY;export TMPDIR=$TMPDIR;export XDG_RUNTIME_DIR=$XDG_RUNTIME_DIR;module load cloudcompare;CloudCompare"'
<%else%>
Exec=ml cloudcompare;CloudCompare
<%end%>
Icon=<%= File.join(session.staged_root, "icons", "cloudcompare.png") -%>
